<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>Emre Üçbudak</Authors>
    <Version>1.0.0</Version>
    <Title>FlashMediator</Title>
    <Company>Emre Üçbudak</Company>
    <Description>High performance Mediator library</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageIcon>flashmediator.jpg</PackageIcon>
    <RepositoryUrl>https://github.com/emreucbudak/FlashMediator</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="10.0.1" />
  </ItemGroup>

	<ItemGroup>
		<None Include="src\flashmediator.jpg" Pack="True" PackagePath="" />
	</ItemGroup>

</Project>
